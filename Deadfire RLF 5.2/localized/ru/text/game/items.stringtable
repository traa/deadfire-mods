      <ID>111</ID>
      <DefaultText>Изумруд</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>112</ID>
      <DefaultText>Гранат</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>115</ID>
      <DefaultText>Та ондра тара</DefaultText>
      <FemaleText />
