      <ID>972</ID>
      <DefaultText>Ортйома</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>973</ID>
      <DefaultText>Ратунский</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>974</ID>
      <DefaultText>Рауатайский</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
